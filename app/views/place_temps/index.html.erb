<p id="notice"><%= notice %></p>

<h1>Place Temps</h1>

<table>
  <thead>
    <tr>
      <th>Place</th>
      <th>Date</th>
      <th>Temp</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @place_temps.each do |place_temp| %>
      <tr>
        <td><%= place_temp.place %></td>
        <td><%= place_temp.date %></td>
        <td><%= place_temp.temp %></td>
        <td><%= link_to 'Show', place_temp %></td>
        <td><%= link_to 'Edit', edit_place_temp_path(place_temp) %></td>
        <td><%= link_to 'Destroy', place_temp, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Place Temp', new_place_temp_path %>
